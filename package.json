{
  "name": "@modyqyw/utils",
  "version": "0.0.0",
  "description": "Shareable utils for different JavaScript/TypeScript projects.",
  "author": {
    "email": "wurui7@mail3.sysu.edu.cn",
    "name": "ModyQyW",
    "url": "https://modyqyw.github.io"
  },
  "private": false,
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.min.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.min.js",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.min.js",
      "require": "./dist/index.cjs.min.js"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ModyQyW/utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/utils.git"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script",
    "lint:json": "prettier ./**/*.json --write",
    "lint:markdown": "markdownlint . --fix && lint-md . --fix",
    "lint:script": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "commit": "cz",
    "prepare": "is-ci || husky install",
    "release": "np --no-release-draft --message=chore(release): v%s",
    "test": "yarn run lint && jest --coverage",
    "version": "yarn build"
  },
  "dependencies": {
    "change-case": "~4.1.2",
    "convert": "~1.9.1",
    "dayjs": "~1.10.5",
    "faker": "~5.5.3",
    "format-rmb": "~1.2.3",
    "gcoord": "~0.3.2",
    "lodash-es": "~4.17.21",
    "numbro": "~2.3.2",
    "path-to-regexp": "~6.2.0",
    "qs": "~6.10.1",
    "ua-parser-js": "~0.7.28",
    "uuid": "~8.3.2",
    "validator": "~13.6.0"
  },
  "devDependencies": {
    "@babel/core": "~7.14.3",
    "@babel/eslint-parser": "~7.14.4",
    "@commitlint/cli": "~12.1.4",
    "@modyqyw/fabric": "~2.2.0",
    "@rollup/plugin-node-resolve": "~13.0.0",
    "@types/faker": "~5.5.6",
    "@types/jest": "~26.0.23",
    "@types/lodash-es": "~4.17.4",
    "@types/node": "~15.6.1",
    "@types/qs": "~6.9.6",
    "@types/ua-parser-js": "~0.7.36",
    "@types/uuid": "~8.3.0",
    "@types/validator": "~13.1.3",
    "@typescript-eslint/eslint-plugin": "~4.26.0",
    "@typescript-eslint/parser": "~4.26.0",
    "eslint": "~7.27.0",
    "husky": "~6.0.0",
    "is-ci": "~3.0.0",
    "jest": "~27.0.3",
    "lint-md-cli": "~0.1.2",
    "lint-staged": "~11.0.0",
    "markdownlint-cli": "~0.27.1",
    "np": "~7.5.0",
    "prettier": "~2.3.0",
    "rollup": "~2.50.5",
    "rollup-plugin-dts": "~3.0.2",
    "rollup-plugin-terser": "~7.0.2",
    "rollup-plugin-typescript2": "~0.30.0",
    "ts-jest": "~27.0.2",
    "tslib": "~2.2.0",
    "typescript": "~4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
