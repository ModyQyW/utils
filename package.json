{
  "name": "@modyqyw/utils",
  "version": "4.1.0",
  "private": false,
  "description": "Opinionated shareable utils for different JavaScript/TypeScript projects.",
  "keywords": [
    "javascript",
    "typescript",
    "js",
    "ts",
    "utils",
    "helpers"
  ],
  "homepage": "https://github.com/ModyQyW/utils#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/utils.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "pnpm run build --watch",
    "lint": "run-p lint:eslint lint:markdownlint check:types test:coverage",
    "lint:eslint": "eslint . --fix --cache --no-error-on-unmatched-pattern",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm run build",
    "release": "release-it",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@sindresorhus/is": "^5.3.0",
    "@types/throttle-debounce": "^5.0.0",
    "deepmerge": "^4.3.0",
    "lower-case": "^2.0.2",
    "p-debounce": "^4.0.0",
    "p-limit": "^4.0.0",
    "p-retry": "^5.1.2",
    "p-throttle": "^5.0.0",
    "throttle-debounce": "^5.0.0",
    "title-case": "^3.0.3",
    "ts-extras": "^0.11.0",
    "type-fest": "^3.5.6",
    "upper-case": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt": "^17.4.2",
    "@modyqyw/fabric": "^6.0.6",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "c8": "^7.12.0",
    "change-case": "^4.1.2",
    "commitizen": "^4.3.0",
    "esbuild": "^0.17.6",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.1",
    "markdownlint-cli": "^0.33.0",
    "prettier": "^2.8.4",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.14.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
