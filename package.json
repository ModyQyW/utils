{
  "name": "@modyqyw/utils",
  "version": "3.0.4",
  "private": false,
  "description": "Opinionated utils for JavaScript / TypeScript projects",
  "keywords": [
    "javascript",
    "typescript",
    "js",
    "ts",
    "utils",
    "helpers"
  ],
  "homepage": "https://github.com/ModyQyW/utils#readme",
  "bugs": {
    "url": "https://github.com/ModyQyW/utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ModyQyW/utils.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "pnpm run build --watch",
    "lint": "run-p lint:eslint lint:markdownlint check:types test:coverage",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@sindresorhus/is": "^5.3.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.throttle": "^4.1.7",
    "change-case": "^4.1.2",
    "deepmerge-ts": "^4.2.2",
    "esbuild": "^0.16.13",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "lower-case": "^2.0.2",
    "nanoid": "^4.0.0",
    "p-retry": "^5.1.2",
    "rollup": "^3.9.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^5.1.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-hashbang": "^3.0.0",
    "terser": "^5.16.1",
    "title-case": "^3.0.3",
    "type-fest": "^3.5.0",
    "upper-case": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt": "^17.3.0",
    "@modyqyw/fabric": "^5.14.0",
    "@tsconfig/node18": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vitest/ui": "^0.26.3",
    "c8": "^7.12.0",
    "commitizen": "^4.2.6",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "prettier": "^2.8.1",
    "release-it": "^15.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "vitest": "^0.26.3"
  },
  "packageManager": "pnpm@7.21.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
